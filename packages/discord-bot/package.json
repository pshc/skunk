{
  "name": "skunk-discord-bot",
  "version": "1.0.1",
  "description": "Discord game bot",
  "private": true,
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc --build --noEmit",
    "test": "tsc --build && tsc -p test.tsconfig.json && ava .test/discord-bot/test/index.js",
    "start": "tsc --build && node deploy-commands && node dist/index.js",
    "clean": "rm -f -- rolls_test*.csv",
    "distclean": "rm -rf -- dist .test"
  },
  "author": "uweng",
  "license": "ISC",
  "dependencies": {
    "@discordjs/builders": "0.13",
    "@discordjs/rest": "0.4",
    "burrow": "workspace:*",
    "discord-api-types": "0.33",
    "discord.js": "13",
    "dotenv": "16",
    "ioredis": "5"
  },
  "devDependencies": {
    "@ava/typescript": "3",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "ava": "4",
    "eslint": "8",
    "typescript": "4"
  },
  "engines": {
    "node": ">=16"
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 100
  },
  "ava": {
    "environmentVariables": {
      "FAST_EMOJI": "."
    }
  },
  "imports": {
    "#burrow/*": "burrow/*"
  }
}
